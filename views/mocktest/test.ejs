<script type="text/javascript">
  
$(document).ready(function(){

  
  $('#mocktesttab').click(function(event){                  
                  $('#mocktesttab').removeClass('active');
                  $('#paymenttab').removeClass('active');

                  $('#mocktesttab').addClass('active');

                  $('#main_panel').show();
                  $('#payment_panel').hide();
  });


  $('#paymenttab').click(function(event){                  
                  $('#mocktesttab').removeClass('active');
